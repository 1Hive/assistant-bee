{
  "name": "bee-guard",
  "version": "0.0.1",
  "description": "A discord bot to regulate BrightID links on public channels.",
  "keywords": [
    "discord",
    "1hive",
    "brightid"
  ],
  "main": "src/index.js",
  "scripts": {
    "lint": "npx eslint ./src",
    "lint-fix": "npx eslint ./src --fix",
    "test": "npx jest --passWithNoTests",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js"
  },
  "author": "1Hive",
  "license": "MIT",
  "dependencies": {
    "@aragon/connect-thegraph": "^0.7.0",
    "@sentry/node": "^5.15.4",
    "discord.js": "^12.4.0",
    "dotenv": "^8.2.0",
    "graphql-tag": "^2.11.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "sourcecred": "^0.7.1",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npx eslint --fix"
    ]
  }
}
